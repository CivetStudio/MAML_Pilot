<Template>

	<Props>
		<item name="condition" type="string" description="条件判断" default="$#tc$" />
	</Props>

	<!-- 视频 -->
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.miui.video" class="com.miui.video.HomeActivity" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.android.VideoPlayer" class="com.vivo.video.app.splash.SplashActivity" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.android.VideoPlayer" class="com.android.VideoPlayer.VideoPlayer" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.coloros.yoli" class="com.coloros.yoli.MainActivity" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.coloros.yoli" class="com.coloros.yoli.maintab.ui.SplashActivity" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.coloros.yoli" class="com.coloros.yoli.icon_org" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.heytap.yoli" class="com.heytap.yoli.icon_org" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.hihonor.youku.video" class="com.youku.v2.HomePageEntry" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.huawei.himovie" class="com.huawei.himovie.ui.login.SplashScreenActivity" />

</Template>