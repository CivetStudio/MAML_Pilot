<Template>

	<Props>
		<item name="condition" type="string" description="条件判断" default="$#tc$" />
	</Props>

	<!-- 相机 -->
	<IntentCommand condition="($condition)*ne($#LangsId$,2)" action="android.intent.action.MAIN" package="com.android.camera" class="com.android.camera.Camera" />
	<IntentCommand condition="($condition)*ne($#LangsId$,2)" action="android.intent.action.MAIN" package="com.coloros.camera" class="com.coloros.camera.Camera" />
	<IntentCommand condition="($condition)*ne($#LangsId$,2)" action="android.intent.action.MAIN" package="com.oppo.camera" class="com.oppo.camera.Camera" />
	<IntentCommand condition="($condition)*ne($#LangsId$,2)" action="android.intent.action.MAIN" package="com.oplus.camera" class="com.oplus.camera.Camera" />
	<IntentCommand condition="($condition)*ne($#LangsId$,2)" action="android.intent.action.MAIN" package="com.android.camera" class="com.android.camera.CameraActivity" />
	<IntentCommand condition="($condition)*eq($#LangsId$,2)" action="android.media.action.STILL_IMAGE_CAMERA_SECURE" />
	<ExternCommand condition="($condition)*ne($#LangsId$,2)" command="unlock" />

</Template>