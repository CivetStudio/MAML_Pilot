<Template>

	<Props>
		<item name="condition" type="string" description="条件判断" default="$#tc$" />
	</Props>

	<!-- 浏览器 -->
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.vivo.browser" class="com.vivo.browser.BrowserActivity" /><!-- alias="mCivetCode_5" -->
	<IntentCommand condition="($condition)" action="com.vivo.browser.action.fromtile" package="com.vivo.browser" class="com.vivo.browser.MainActivity" />
	<IntentCommand condition="($condition)*ne(#LangsId,5)" action="android.intent.action.MAIN" package="com.android.browser" class="com.android.browser.BrowserActivity" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.nearme.browser" class="com.android.browser.BrowserActivity" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.coloros.browser" class="com.android.browser.BrowserActivity" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.heytap.browser" class="com.android.browser.BrowserActivity" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.hihonor.browser" class="com.hihonor.browser.Main2" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.hihonor.baidu.browser" class="com.baidu.searchbox.SplashActivity" />
	<IntentCommand condition="($condition)" action="android.intent.action.MAIN" package="com.huawei.browser" class="com.huawei.browser.Main2" />

</Template>